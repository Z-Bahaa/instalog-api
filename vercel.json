{
  "rewrites": [{ "source": "(.*)", "destination": "/api" }],
  "version": 2,
  "buildCommand": "tsc",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "outputDirectory": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}